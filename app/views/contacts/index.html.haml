%h3 My Contacts

.col-md-12
  %table.table.table-striped#contacts{data: {src: contacts_path}}
    %thead.bg-primary
      %tr
        %th First Name
        %th Last Name
        %th Email
        %th Phone
        %th Company
        %th Actions


%hr.col-md-12

.col-md-12
  .well.col-md-12
    %h3 Add Contact

    = render partial: 'contacts/form', locals: {contact: @contact}
